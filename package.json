{
  "name": "angular-persistence",
  "version": "1.0.0-3",
  "description": "A library to handle persistence for Angular 2 applications.",
  "main": "bundles/angular_persistence.umd.js",
  "module": "index.js",
  "scripts": {
    "build": "node build.js",
    "debug": "karma start karma.conf.js --browsers=Chrome --single-run=false",
    "test": "karma start",
    "pack-lib": "npm pack ./dist",
    "publish-lib": "npm publish ./dist",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc-serve": "compodoc -s"
  },
  "typings": "index.d.ts",
  "author": "Scott O'Bryan",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkarena1/angular-persistence"
  },  
  "keywords": [
    "angular",
    "ngstorage",
    "persistence",
    "browser storage",
    "web storage",
    "session storage",
    "local storage",
    "angular storage"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": ">= 2.4.6 || >= 4.0.0-rc.6",
    "@angular/core": ">= 2.4.6 || >= 4.0.0-rc.6",
    "@angular/router": ">=3.4.6 || >= 4.0.0-rc.6"
  },
  "devDependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/compiler-cli": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/platform-server": "2.4.6",
    "@angular/router": "3.4.6",
    "@types/jasmine": "2.5.41",
    "@types/node": "7.0.5",
    "chalk": "1.1.3",
    "codelyzer": "2.0.0",
    "compodoc": "0.0.41",
    "core-js": "2.4.1",
    "jasmine-core": "2.5.2",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "2.0.2",
    "reflect-metadata": "0.1.9",
    "rollup": "0.41.4",
    "rxjs": "5.1.0",
    "shelljs": "0.7.6",
    "ts-helpers": "1.1.2",
    "ts-loader": "2.0.0",
    "tslint": "4.4.2",
    "typescript": "2.0.10",
