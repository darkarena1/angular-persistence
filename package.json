{
  "name": "angular-persistence",
  "version": "1.1.0-beta.0",
  "description": "A library to handle persistence for Angular 2 applications.",
  "main": "bundles/angular_persistence.umd.js",
  "module": "index.js",
  "scripts": {
    "build": "node build.js",
    "debug": "karma start karma.conf.js --browsers=Chrome --single-run=false",
    "test": "karma start",
    "pack-lib": "npm pack ./dist",
    "publish-lib": "npm publish ./dist",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc-serve": "compodoc -s"
  },
  "typings": "index.d.ts",
  "author": "Scott O'Bryan",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkarena1/angular-persistence"
  },
  "keywords": [
    "angular",
    "ngstorage",
    "persistence",
    "browser storage",
    "web storage",
    "session storage",
    "local storage",
    "angular storage"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "~5.0.0",
    "@angular/compiler": "~5.0.0",
    "@angular/compiler-cli": "~5.0.0",
    "@angular/core": "~5.0.0",
    "@angular/platform-browser": "~5.0.0",
    "@angular/platform-browser-dynamic": "~5.0.0",
    "@angular/platform-server": "~5.0.0",
    "@angular/router": "~5.0.0",
    "@types/jasmine": "2.5.53",
    "@types/node": "6.0.60",
    "chalk": "^2.3.0",
    "codelyzer": "4.0.1",
    "compodoc": "0.0.41",
    "core-js": "2.4.1",
    "jasmine-core": "2.6.2",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "2.0.9",
    "reflect-metadata": "0.1.12",
    "rollup": "0.54.1",
    "rxjs": "5.5.2",
    "shelljs": "0.8.0",
    "ts-helpers": "1.1.2",
    "ts-loader": "3.2.0",
	  "ts-node": "3.2.0",
    "tslint": "5.7.0",
    "typescript": "2.6.2",
    "uglify-js": "3.3.7",
    "webpack": "3.10.0",
    "zone.js": "0.8.14"
